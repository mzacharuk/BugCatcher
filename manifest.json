  {
    "name": "BugCatcher - You gotta catch'em all",
    "description" : "Report bugs faster!",
	"background": {
		"persistent": false,
		"scripts": ["background.js"]
	},
    "version": "0.1",
    "manifest_version": 2,
	"icons": {
	"128": "128.png"
	},
    "browser_action": {
      "default_icon": "128.png"
    },
	"permissions": [
    "activeTab"
	],
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+F",
          "mac": "MacCtrl+Shift+F"
        },
        "description": "Opens new tab with screenshot and form to bug report."
      }
    }
  
  }